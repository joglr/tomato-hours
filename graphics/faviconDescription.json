{
	"masterPicture": "graphics/tomato-hours-plate.svg",
	"iconsPath": "/",
	"design": {
		"ios": {
			"pictureAspect": "backgroundAndMargin",
			"backgroundColor": "#f3f3f3",
			"margin": "0%",
			"assets": {
				"ios6AndPriorIcons": false,
				"ios7AndLaterIcons": false,
				"precomposedIcons": false,
				"declareOnlyDefaultIcon": true
			}
		},
		"desktopBrowser": {},
		"windows": {
			"masterPicture": "graphics/tomato-hours-outline.svg",
			"pictureAspect": "noChange",
			"backgroundColor": "#f44336",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": false,
				"windows10Ie11EdgeTiles": {
					"small": false,
					"medium": true,
					"big": false,
					"rectangle": false
				}
			}
		},
		"androidChrome": {
			"pictureAspect": "noChange",
			"themeColor": "#f44336",
			"manifest": {
				"name": "Tomato Hours",
				"startUrl": "/",
				"display": "standalone",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": false,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"masterPicture": "graphics/tomato-hours-outline.svg",
			"pictureAspect": "silhouette",
			"themeColor": "#f44336"
		}
	},
	"settings": {
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false,
		"readmeFile": false,
		"htmlCodeFile": false,
		"usePathAsIs": false
	}
}
