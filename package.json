  "scripts": {
    "start": "react-scripts start",
    "build": "rimraf build && react-scripts build && npm run icon",
    "serve-build": "npm run build && serve build",
    "deploy": "now --public && now alias tomato-hours",
    "test": "cross-env CI=true react-scripts test --env=jsdom --verbose",
    "test-watch": "react-scripts test --env=jsdom --verbose",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "icon": "real-favicon generate './graphics/faviconDescription.json' './graphics/faviconData.json' 'build/static/media' && real-favicon inject './graphics/faviconData.json' './build' './build/index.html'",
    "check-icon-update": "real-favicon check-for-update --fail-on-update './graphics/faviconData.json'"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^1.1.0",
    "firebase": "^4.1.3",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.22.2",
    "prop-types": "^15.6.1",
    "raven-js": "^3.16.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-ga": "^2.4.1",
    "react-md": "^1.2.11",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-timer-middleware": "^0.1.2",
    "roboto-fontface": "^0.9.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cli-real-favicon": "0.0.6",
    "cross-env": "^5.1.3",
    "eslint-plugin-jest": "^21.12.1",
    "eslint-plugin-react": "^7.6.1",
    "react-scripts": "^1.1.4",
    "rimraf": "^2.6.2"
  }
}
