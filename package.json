    "prop-types": "^15.7.2",
    "raven-js": "^3.27.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-md": "^1.14.1",
    "react-redux": "^5.1.2",
    "redux": "^4.0.5",
    "redux-timer-middleware": "^1.0.0",
    "roboto-fontface": "^0.10.0"
  },
  "devDependencies": {
    "cli-real-favicon": "0.0.8",
